 &PFInversion

!#################################################################
! XTAL NAME 
!#################################################################

 xtalname = 'undefined',

!#################################################################
! Miller Indices and respective files; right now it assumes 
! a miximum of 10 pole figures
! all Miller indices in 3 index notation
!#################################################################

! nfiles
 nfiles = 3,
 flist = 'filename1.txt', 'filename2.txt','filename3.txt',

!#################################################################
! regularization paramter for L2 regularization
!#################################################################

 damp = 1.D0,

