!#################################################################
! number of points in lambert projections
! size of PF data is 2nLam+1**2
 nLam = 20,
! number of points in discrete ODF
! size of ODF is 2*ncub+1**3
 ncub = 40,
! output data file; relative to EMdatapathname
 datafile = 'undefined'
! mrc file name for visualization
 mrcfile = 'undefined'
 /
